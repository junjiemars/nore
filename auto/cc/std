#### -*- mode:sh -*- vim:ft=sh
####
## No More than a C build system for clang, gcc and msvc.
## https://github.com/junjiemars/nore
####
## cc/std
####


if [ "NO" = "$NM_STD" ]; then
  :

elif [ "YES" = "$NM_STD" ]; then
  nm_std_opt="${std_opt}c99"

else
  nm_std_opt="${std_opt}$NM_STD"

fi


if [ "NO" != "$NM_STD" -a -n "$nm_std_opt" ]; then
	CFLAGS=${CFLAGS:+$CFLAGS }${nm_std_opt}
fi



