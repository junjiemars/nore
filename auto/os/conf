# echo "checking for $NM_PLATFORM specific features"

case "$NM_SYSTEM" in
  Linux) . ${NORE_ROOT}/auto/os/linux ;;
  Darwin) . ${NORE_ROOT}/auto/os/darwin ;;
  WinNT) . ${NORE_ROOT}/auto/os/nt ;;
esac


# eof
